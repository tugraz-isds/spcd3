{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "spcd3",
  "version": "1.0.0",
  "identifier": "com.spcd3.dev",
  "build": {
    "frontendDist": "../dist/example",
    "devUrl": "http://localhost:3000/",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "spcd3",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
